{
	"root": true,
	"extends": ["plugin:@wordpress/eslint-plugin/recommended"],
	"env": {
		"browser": true,
		"jquery": true
	},
	"globals": {
		"wp": "readonly",
		"ajaxurl": "readonly",
		"booknow_admin": "readonly",
		"booknow_public": "readonly",
		"Stripe": "readonly"
	},
	"rules": {
		"no-console": "warn",
		"no-unused-vars": "warn"
	}
}
